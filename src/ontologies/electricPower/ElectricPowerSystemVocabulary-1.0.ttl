# Electrical connection point 

seas:DirectCurrentConnectionPoint a owl:Class ;
  rdfs:label "Direct Current Connection Point"@en ;
  rdfs:comment "The class of electrical connection points through which energy flows using direct current."@en ;
  rdfs:subClassOf seas:ElectricalConnectionPoint ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

  seas:TwoWireDirectCurrentConnectionPoint a owl:Class ;
    rdfs:label "Two Wire Direct Current Connection Point"@en ;
    rdfs:comment """A two wire direct current connection point is a connection point composed of two wires (potentialy plus the protective earth): 
     wire for the positive;
     wire for the negative."""@en ;
    rdfs:subClassOf seas:DirectCurrentConnectionPoint ;  
    vs:term_status "testing" ;
    rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

  seas:ThreeWireDirectCurrentConnectionPoint a owl:Class ;
    rdfs:label "Three Wire Direct Current Connection Point"@en ;
    rdfs:comment """A three wire direct current power connection point is a connection point composed of three wires (potentialy plus the protective earth): 
     wire for the positive;
     wire for the mid-wire;
     wire for the negative."""@en ;
    rdfs:subClassOf seas:DirectCurrentConnectionPoint ;  
    vs:term_status "testing" ;
    rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

seas:AlternatingCurrentConnectionPoint a owl:Class ;
  rdfs:label "Alternating Current Connection Point"@en ;
  rdfs:comment "The class of electrical power connection points through which energy flows using alternating current."@en ;
  rdfs:subClassOf seas:ElectricalConnectionPoint ;
  owl:disjointWith seas:DirectCurrentConnectionPoint ;
  rdfs:subClassOf [ owl:onProperty seas:voltage ; owl:allValuesFrom seas:PeriodicProperty ] ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

  seas:ThreePhaseConnectionPoint a owl:Class ;
    rdfs:label "Three Phase Connection Point"@en ;
    rdfs:comment """A three phase connection point is a connection point composed of four wires (plus the protective earth): 
     wires R, S, T, for the phases;
     wire N for the neutral."""@en ;
    rdfs:subClassOf seas:AlternatingCurrentConnectionPoint ;  
    vs:term_status "testing" ;
    rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

    seas:connectsSystemInStarAt a owl:ObjectProperty ;
      rdfs:label "connects system in star at"@en ;
      rdfs:comment "Links a three phase power bus to one of the three phase connection points it connects with a star configuration."@en ;
      rdfs:domain seas:ThreePhasePowerBus ;
      rdfs:range seas:ThreePhaseConnectionPoint ;
      owl:inverseOf seas:connectsSystemInStarThrough ;
      vs:term_status "stable" ;
      rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

    seas:connectsSystemInStarThrough a owl:ObjectProperty ;
      rdfs:label "connects system in star through"@en ;
      rdfs:comment "Links a three phase connection point to a three phase power bus connection with which it is connected with a star configuration."@en ;
      rdfs:domain seas:ThreePhaseConnectionPoint ;
      rdfs:range seas:ThreePhasePowerBus ;
      owl:inverseOf seas:connectsSystemInStarAt ;
      vs:term_status "stable" ;
      rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

    seas:connectsSystemInTriangleAt a owl:ObjectProperty ;
      rdfs:label "connects system in triangle at"@en ;
      rdfs:comment "Links a three phase power bus to one of the three phase connection points it connects with a triangle configuration."@en ;
      rdfs:domain seas:ThreePhasePowerBus ;
      rdfs:range seas:ThreePhaseConnectionPoint ;
      owl:inverseOf seas:connectsSystemInTriangleThrough ;
      vs:term_status "stable" ;
      rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

    seas:connectsSystemInTriangleThrough a owl:ObjectProperty ;
      rdfs:label "connects system in triangle through"@en ;
      rdfs:comment "Links a three phase connection point to a three phase power bus connection with which it is connected with a triangle configuration."@en ;
      rdfs:domain seas:ThreePhaseConnectionPoint ;
      rdfs:range seas:ThreePhasePowerBus ;
      owl:inverseOf seas:connectsSystemInTriangleAt ;
      vs:term_status "stable" ;
      rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

    seas:connectsSystemInStarThrough owl:propertyDisjointWith seas:connectsSystemInTriangleThrough .

  seas:SinglePhaseConnectionPoint a owl:Class ;
    rdfs:label "Single Phase Connection Point"@en ;
    rdfs:comment """A single phase connection point is a connection point composed of two wires (plus the protective earth): 
     wire for the phase;
     wire for the neutral."""@en ;
    rdfs:subClassOf seas:AlternatingCurrentConnectionPoint ;  
    vs:term_status "testing" ;
    rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

    seas:connectsSystemInRNAt a owl:ObjectProperty ;
