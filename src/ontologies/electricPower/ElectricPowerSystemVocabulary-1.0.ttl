      owl:inverseOf seas:connectsSystemInSNThrough ;
      vs:term_status "stable" ;
      rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

    seas:connectsSystemInSNThrough a owl:ObjectProperty ;
      rdfs:label "connects system in SN through"@en ;
      rdfs:comment "Links a single phase connection point to a three phase power bus connection with which it is connected with a SN configuration."@en ;
      rdfs:domain seas:SinglePhaseConnectionPoint ;
      rdfs:range seas:ThreePhasePowerBus ;
      owl:inverseOf seas:connectsSystemInSNAt ;
      vs:term_status "stable" ;
      rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

    seas:connectsSystemInTNAt a owl:ObjectProperty ;
      rdfs:label "connects system in TN at"@en ;
      rdfs:comment "Links a three phase power bus to one of the single phase connection points it connects with a TN configuration."@en ;
      rdfs:domain seas:ThreePhasePowerBus ;
      rdfs:range seas:SinglePhaseConnectionPoint ;
      owl:inverseOf seas:connectsSystemInTNThrough ;
      vs:term_status "stable" ;
      rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

    seas:connectsSystemInTNThrough a owl:ObjectProperty ;
      rdfs:label "connects system in TN through"@en ;
      rdfs:comment "Links a single phase connection point to a three phase power bus connection with which it is connected with a TN configuration."@en ;
      rdfs:domain seas:SinglePhaseConnectionPoint ;
      rdfs:range seas:ThreePhasePowerBus ;
      owl:inverseOf seas:connectsSystemInTNAt ;
      vs:term_status "stable" ;
      rdfs:isDefinedBy seas:ElectricPowerSystemVocabulary .

    seas:connectsSystemInRNThrough owl:propertyDisjointWith seas:connectsSystemInSNThrough .
