  skos:notation "Replace" ;
  rdfs:comment """Estimates the replacement of the feature of interest (timestamp, duration, cost,...)."""@en ;
  skos:definition """Estimates the replacement of the feature of interest (timestamp, duration, cost,...)."""@en ;
