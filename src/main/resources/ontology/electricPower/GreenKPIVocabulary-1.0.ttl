seas:GreenKPIVocabulary rdf:type voaf:Vocabulary , owl:Ontology ;
  dc:title "SEAS-GreenKPI vocabulary"@en ;
  dc:description """The SEAS Green KPI Vocabulary defines KPIs for the energy domain."""@en ;
  dc:issued "2016-06-06"^^xsd:date ;
  dc:modified "2016-07-26"^^xsd:date ;
  dc:author <http://www.maxime-lefrancois.info/me#> ;
  dc:contributor [ a foaf:Person ; foaf:name "Lamya Belhaj" ] ;
  vann:preferredNamespacePrefix "seas" ;
  vann:preferredNamespaceUri <https://w3id.org/seas/> ;  
#  owl:imports seas:ElectricPowerSystemOntology ;
  owl:versionIRI <https://w3id.org/seas/GreenKPIVocabulary/1.0> ;
  owl:versionInfo "v1.0" .

# CL_GREENKPI
seas:greenkpi a skos:ConceptScheme, sdmx:CodeList;
  rdfs:label "Green KPI - codelist scheme"@en;
  skos:prefLabel "Green KPI - codelist scheme"@en;
  skos:notation "CL_GREENKPI";
  rdfs:comment """This code list enables to interpret the given value with respect to green KPIs."""@en;
  skos:definition seas:tp ;
  rdfs:seeAlso seas:Tp ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:GreenKPIVocabulary .

seas:Greenkpi a rdfs:Class, owl:Class;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Green KPI - codelist class"@en;
  rdfs:comment """This code list enables to interpret the given value with respect to green KPIs."""@en;
  rdfs:seeAlso seas:tp ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:GreenKPIVocabulary . 

seas:greenkpi-local a skos:Concept, seas:EvaluationInterpretationConcept, seas:Tp;
  rdfs:label "local"@en ;
  skos:prefLabel "local"@en ;
  skos:notation "local" ;
  rdfs:comment """Percentage of the property that is local."""@en ;
  skos:topConceptOf seas:greenkpi;
  skos:inScheme   seas:greenkpi;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:GreenKPIVocabulary . 

seas:greenkpi skos:hasTopConcept seas:greenkpi-greenLocal .


seas:greenkpi-carbonFootprint a skos:Concept, seas:EvaluationInterpretationConcept, seas:Tp;
  rdfs:label "carbon footprint"@en ;
  skos:prefLabel "carbon footprint"@en ;
  skos:notation "carbon footprint" ;
  rdfs:comment """carbon footprint of the property."""@en ;
  skos:topConceptOf seas:greenkpi;
  skos:inScheme   seas:greenkpi;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:GreenKPIVocabulary . 

seas:greenkpi skos:hasTopConcept seas:greenkpi-carbonFootprint .

