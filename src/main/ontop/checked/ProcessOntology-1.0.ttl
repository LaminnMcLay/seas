seas:ProcessOntology rdf:type voaf:Vocabulary , owl:Ontology ;
  dc:title "The SEAS Process ontology"@en ;
  dc:description """The SEAS Process ontology describes different classes of processes.

The main class process is [`pep:Process`](https://w3id.org/pep/Process) taken from the [Process Execution Platform (PEP) ontology](https://w3id.org/pep/)."""@en ;
  dc:issued "2016-09-26"^^xsd:date ;
  dc:modified "2016-09-26"^^xsd:date ;
  dc:creator <http://www.maxime-lefrancois.info/me#> ;
  cc:license <https://www.apache.org/licenses/LICENSE-2.0> ;
  vann:preferredNamespacePrefix "seas" ;
  vann:preferredNamespaceUri <https://w3id.org/seas/> ;  
  owl:imports pep: ;
  owl:versionIRI <https://w3id.org/seas/ProcessOntology-1.0> ;
  owl:versionInfo "v1.0" .

seas:ForecastingProcess a owl:Class ; 
  rdfs:label "ForecastingProcess"@en ;
  rdfs:comment "Process whose output describes some forecast for some property."@en ;
  rdfs:subClassOf pep:Process;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:ActorOntology.
  
