seas:FourierVocabulary a owl:Ontology , voaf:Vocabulary ;
  dc:title "SEAS-Fourier Vocabulary."@en ;
  dc:description "This ontology defines a set of SKOS Concepts and ConceptSchemes for code lists. These concepts are used as metadata on instances of seas:Evaluation and enable to interpret evaluations of quantifiable properties."@en ;
  dc:issued "2016-07-05"^^xsd:date ;
  dc:modified "2016-07-24"^^xsd:date ;
  dc:creator <http://www.maxime-lefrancois.info/me#> ;
  cc:license <https://www.apache.org/licenses/LICENSE-2.0> ;
  vann:preferredNamespacePrefix "seas" ;
  vann:preferredNamespaceUri <https://w3id.org/seas/> ;  
  owl:versionIRI <https://w3id.org/seas/FourierVocabulary-0.10> ;
  owl:versionInfo "v0.10" . 


seas:PeriodicProperty a owl:Class ;
  rdfs:label "Periodic Property"@en ;
  rdfs:comment "The property is a periodic signal."@en ;
  rdfs:subClassOf [
    owl:intersectionOf (
      seas:Property 
      rdfs:subClassOf [ 
        owl:onProperty seas:Evaluation ; 
        owl:allValuesFrom [ 
          owl:onProperty seas:interpretedAs ; 
          owl:someValuesFrom seas:Fourier ] ] ) ].

# code lists


# CL_FOURIER


seas:frequency a owl:ObjectProperty , owl:FunctionalProperty ;
  rdfs:label "frequency"@en ;
  rdfs:comment """Frequency is the property means that the given value measures the frequency of the periodic signal."""@en ;
  rdfs:subPropertyOf seas:hasProperty ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:FourierVocabulary . 

seas:PeakEvaluation a TODO ;
  rdfs:label "Peak"@en ;
  skos:prefLabel "Peak"@en ;
  rdfs:comment """Peak means that the given value measures the Peak of the periodic signal."""@en ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:FourierVocabulary . 

seas:THDEvaluation a owl:Class ;
  rdfs:label "Total Harmonic Distortion"@en ;
  rdfs:comment """THD means that the given value measures the total harmonic distortion of the periodic signal."""@en ;
  rdfs:subClassOf seas:Evaluation ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:FourierVocabulary . 


<fridge/1> a seas:ElectricPowerSystem ;
  seas:voltage <fridge/1/voltage> .

<fridge/1/voltage> a seas:VoltageProperty ;
  seas:evaluation <fridge/1/voltage/evaluation/1> .

<fridge/1/voltage/evaluation/1> a seas:THDEvaluation ;
  seas:evaluatedValue "123 V"^^cdt:ucum .









seas:fourier-RMS a TODO ;
  rdfs:label "RMS Amplitude"@en ;
  skos:prefLabel "RMS Amplitude"@en ;
  rdfs:comment """RMS means that the given value measures the Root Mean Square amplitude of the periodic signal."""@en ;
