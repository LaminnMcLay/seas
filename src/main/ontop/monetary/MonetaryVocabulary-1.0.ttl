seas:MonetaryVocabulary rdf:type voaf:Vocabulary , owl:Ontology ;
  dc:title "SEAS-Monetary ontology"@en ;
  dc:description """The SEAS Monetary Vocabulary defines code lists to interpret evaluations of operating features of interest. For instance, the value may be the minimal operating value"""@en ;
  dc:issued "2016-07-24"^^xsd:date ;
  dc:modified "2016-07-24"^^xsd:date ;
  dc:author <http://www.maxime-lefrancois.info/me#> ;
  cc:license <https://creativecommons.org/licenses/by/4.0> ;
  vann:preferredNamespacePrefix "seas" ;
  vann:preferredNamespaceUri <https://w3id.org/seas/> ;  
#  owl:imports seas:EvaluationOntology ;
  owl:versionIRI <https://w3id.org/seas/MonetaryVocabulary-1.0> ;
  owl:versionInfo "v1.0" .

seas:price a skos:Concept, seas:EvaluationInterpretationConcept ;
  rdfs:label "Price"@en ;
  skos:prefLabel "Price"@en ;
  skos:notation "Price" ;
  rdfs:comment """The given value estimates the price of some aspect of the property."""@en ;
  skos:definition """The given value estimates the price of some aspect of the property."""@en ;
  vs:term_status "stable" ;
  rdfs:isDefinedBy seas:MonetaryVocabulary . 
