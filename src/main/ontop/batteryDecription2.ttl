@prefix cdt: <http://w3id.org/lindt/custom_datatypes#>.

@prefix seas: <https://w3id.org/seas/>.
@base <https://w3id.org/seas/>.

voaf:Vocabulary a owl:Class .
dc:title a owl:AnnotationProperty .
dc:description a owl:AnnotationProperty .
dc:issued a owl:AnnotationProperty .
dc:modified a owl:AnnotationProperty .
dc:creator a owl:AnnotationProperty .
dc:contributor a owl:AnnotationProperty .
cc:license a owl:AnnotationProperty .
vann:preferredNamespacePrefix a owl:AnnotationProperty .
vann:preferredNamespaceUri a owl:AnnotationProperty .
vs:term_status a owl:AnnotationProperty .
foaf:Person a owl:Class .
foaf:name a owl:DatatypeProperty .

seas:BatteryOntology rdf:type voaf:Vocabulary , owl:Ontology ;
  dc:title "The SEAS Battery ontology"@en ;
  dc:description """The SEAS Battery ontology describes systems named `seas:Battery`, and their properties."""@en ;
  dc:issued "2016-09-26"^^xsd:date ;
  dc:modified "2016-09-26"^^xsd:date ;
  dc:creator [ foaf:name "Lynda Temal" ] ;
  dc:contributor <http://www.maxime-lefrancois.info/me#> ;
  cc:license <https://www.apache.org/licenses/LICENSE-2.0> ;
  vann:preferredNamespacePrefix "seas" ;
  vann:preferredNamespaceUri <https://w3id.org/seas/> ;  
  owl:imports seas:SystemOntology , seas:ElectricPowerSystemOntology ;
  owl:versionIRI <https://w3id.org/seas/BatteryOntology-0.10> ;
  owl:versionInfo "v0.10" .


seas:Battery a owl:Class;
  rdfs:label "Battery" ;
  rdfs:comment """ """@en ;
  rdfs:subClassOf seas:ElectricPowerStorageSystem.
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:AreaOntology .

seas:ElectricalVehicule a owl:Class;
	rdfs:subClassOf seas:Vehicule.	

seas:stateOfCharge a owl:ObjectProperty, owl:FunctionalProperty ;
	rdfs:subPropertyOf seas:hasProperty;
	rdfs:range seas:EnergyProperty.



<car/1> a seas:FeatureOfInterest, seas:ElectricalVehicule, seas:System ;

<car/1/battery> a seas:FeatureOfInterest, seas:Battery, seas:System;
    seas:subElectricPowerSystemOf <car/1>;
    seas:stateOfCharge <car/1/battery/stateOfCharge>.

<car/1/battery/stateOfCharge> a seas:EnergyProperty ;
  seas:evaluation <car/1/battery/stateOfCharge/minimum> ;
  seas:evaluation <car/1/battery/stateOfCharge/maximum> ;
  seas:evaluation <car/1/battery/stateOfCharge/evaluation/1> .

<car/1/battery/stateOfCharge/minimum> a seas:Evaluation , seas:MinimumEvaluation ;
  seas:evaluatedValue "20 kWh"^^cdt:ucum .

<car/1/battery/stateOfCharge/maximum> a seas:Evaluation , seas:MaximumEvaluation ;
  seas:evaluatedValue "200 kWh"^^cdt:ucum .

<car/1/battery/stateOfCharge/evaluation/1> a seas:Evaluation ;
    prov:wasGeneratedBy <algorithm/1/activity/battery/234> ;
    prov:generatedAtTime "2016-09-12T12:00:00Z"^^xsd:dateTime ;
    seas:hasTemporalContext [ a time:Instant ; time:inXSDDateTime "2016-08-13T12:00:00Z"^^xsd:dateTime ] ;
    seas:evaluatedValue "8.3 kWh"^^cdt:ucum  .  


# l√† c'est un raccourci pour dire que la vitesse de la lumiere est une constante
<Light> a seas:FeatureOfInterest ;
  seas:speed <speedOfLigth> .

<speedOfLight>	a seas:Property ;
  seas:value "300 000 km/s"^^cdt:ucum  .	
