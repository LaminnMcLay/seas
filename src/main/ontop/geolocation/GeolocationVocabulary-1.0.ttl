@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .     #Owl ontology language
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .   #RDF schema 
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .  #XML standard datatypes
@prefix dc: <http://purl.org/dc/terms/> .  #Dublin Core Metadata Initiative terms
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix voaf: <http://purl.org/vocommons/voaf#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix cc: <http://creativecommons.org/ns#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix ssn: <https://www.w3.org/ns/ssn/>.
@prefix cc: <http://creativecommons.org/ns#>.
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#>. # The DOLCE+DnS Ultralite ontology.
@prefix liner: <https://w3id.org/liner/>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix sdmx: <http://purl.org/linked-data/sdmx#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.

@prefix seas: <https://w3id.org/seas/>.
@base <https://w3id.org/seas/>.

seas:GeolocationVocabulary a owl:Ontology , voaf:Vocabulary ;
  dc:title "SEAS-Location Vocabulary."@en ;
  dc:description """This ontology defines:
  - a property keys that enable to qualify the location of a feature of interest, and
  - a SKOS Concept that enables to precise the location context of an evaluation."""@en ;
  dc:issued "2016-07-05"^^xsd:date ;
  dc:modified "2016-07-25"^^xsd:date ;
  dc:creator <http://www.maxime-lefrancois.info/me#> ;
  cc:license <https://creativecommons.org/licenses/by/4.0> ;
  vann:preferredNamespacePrefix "seas" ;
  vann:preferredNamespaceUri <https://w3id.org/seas/> ;  
  owl:imports seas:EvaluationOntology ;
#  owl:imports geo: ; geo: is not a OWL ontology.	 
  owl:versionIRI <https://w3id.org/seas/GeolocationVocabulary-1.0> ;
  owl:versionInfo "v1.0" . 


seas:location a seas:PropertyKey, owl:ObjectProperty , owl:FunctionalProperty ;
  rdfs:label "location" ;
  rdfs:comment """Location property of a feature of interest."""@en ;
  rdfs:subPropertyOf seas:hasProperty ;
  rdfs:range seas:Property ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:GeolocationVocabulary .

seas:geoPoint a owl:ObjectProperty ;
  rdfs:label "geo point"@en ;
  rdfs:comment """Links an evaluation to the geolocalized point at which it is defined."""@en ;
  rdfs:domain seas:Evaluation ;
  rdfs:range geo:Point ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:GeolocationVocabulary .
