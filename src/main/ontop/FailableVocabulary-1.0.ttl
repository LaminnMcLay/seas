voaf:Vocabulary a owl:Class .
dc:title a owl:AnnotationProperty .
dc:description a owl:AnnotationProperty .
dc:issued a owl:AnnotationProperty .
dc:modified a owl:AnnotationProperty .
dc:creator a owl:AnnotationProperty .
dc:contributor a owl:AnnotationProperty .
cc:license a owl:AnnotationProperty .
vann:preferredNamespacePrefix a owl:AnnotationProperty .
vann:preferredNamespaceUri a owl:AnnotationProperty .
vs:term_status a owl:AnnotationProperty .
foaf:Person a owl:Class .
foaf:name a owl:DatatypeProperty .


seas:FailableVocabulary rdf:type voaf:Vocabulary , owl:Ontology ;
  dc:title "SEAS-Failable ontology"@en ;
  dc:description """The SEAS Failable Vocabulary defines property keys and code lists for features of interest operate and/or can can fail."""@en ;
  dc:issued "2016-07-24"^^xsd:date ;
  dc:modified "2016-07-24"^^xsd:date ;
  dc:creator <http://www.maxime-lefrancois.info/me#> ;
  cc:license <https://www.apache.org/licenses/LICENSE-2.0> ;
  vann:preferredNamespacePrefix "seas" ;
  vann:preferredNamespaceUri <https://w3id.org/seas/> ;  
#  owl:imports seas:EvaluationOntology ;
  owl:versionIRI <https://w3id.org/seas/FailableVocabulary-1.0> ;
  owl:versionInfo "v1.0" .


# properties

seas:failure a owl:ObjectProperty , owl:FunctionalProperty ;
  rdfs:label "lenfailuregth" ;
  rdfs:comment """Failure qualifies the failures of a feature of interest."""@en ;
  rdfs:subPropertyOf seas:hasProperty ;
  rdfs:domain seas:FeatureOfInterest ;
  rdfs:range seas:Property ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:FailableVocabulary .

# code lists

# CL_FAIL
seas:fail a skos:ConceptScheme, sdmx:CodeList;
  rdfs:label "Fail - codelist scheme"@en;
  skos:prefLabel "Fail - codelist scheme"@en;
  rdfs:comment """Concepts used to characterise the failure property, and more precisely failures, repairs, replacements, checkups, unavailabilities."""@en;
  skos:note """Concepts used to characterise the failure property, and more precisely failures, repairs, replacements, checkups, unavailabilities."""@en;
  rdfs:seeAlso seas:Failure ;
  vs:term_status "test" ;
  rdfs:isDefinedBy seas:FailableVocabulary .

seas:Failure a rdfs:Class, owl:Class;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Failure - codelist class"@en;
  rdfs:comment """Concepts used to characterise the failure property, and more precisely failures, repairs, replacements, checkups, unavailabilities."""@en;
  rdfs:seeAlso seas:failure ;
  vs:term_status "test" ;
  rdfs:isDefinedBy seas:FailableVocabulary . 

